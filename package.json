{
  "name": "quackr",
  "version": "1.0.0",
  "description": "social media platform for waterfowl",
  "main": "server.ts",
  "scripts": {
    "start": "cross-env NODE_ENV=development DEBUG_MODE='' nodemon --watch server.ts --watch lib --watch app.ts --watch db --watch src --watch routes",
    "test": "cross-env NODE_ENV=test DEBUG_MODE='' jest --runInBand --watchAll --detectOpenHandles",
    "testonce": "cross-env NODE_ENV=test jest",
    "build-dev": "webpack --mode development --watch",
    "build-prod": "webpack --mode production --watch",
    "test:ci": "cross-env NODE_ENV=test jest --runInBand --env=jsdom --detectOpenHandles",
    "build:ci": "npx webpack build --mode production",
    "start-prod": "cross-env NODE_ENV=production node server.js"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/NylonEric/quackr.git"
  },
  "author": "NylonEric",
  "license": "UNLICENSED",
  "bugs": {
    "url": "https://github.com/NylonEric/quackr/issues"
  },
  "homepage": "https://github.com/NylonEric/quackr#readme",
  "dependencies": {
    "@babel/core": "^7.15.5",
    "@babel/eslint-parser": "^7.16.0",
    "@babel/plugin-transform-runtime": "^7.17.0",
    "@babel/preset-env": "^7.15.6",
    "@babel/preset-react": "^7.14.5",
    "@babel/preset-typescript": "^7.21.5",
    "@babel/runtime": "^7.17.2",
    "babel-loader": "^9.1.2",
    "cross-env": "^7.0.3",
    "dotenv": "^16.0.0",
    "express": "^4.17.3",
    "http": "^0.0.1-security",
    "nodemon": "^2.0.15",
    "path": "^0.12.7",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "reload": "^3.2.0",
    "socket.io": "^4.4.1",
    "socket.io-client": "^4.4.1",
    "ts-node": "^10.9.1",
    "ts-node-dev": "^2.0.0"
  },
  "devDependencies": {
    "@types/react": "^18.2.0",
    "eslint": "^7.32.0",
    "eslint-plugin-jest": "^25.2.3",
    "eslint-plugin-react": "^7.26.1",
    "ts-loader": "^9.4.2",
    "webpack": "^5.81.0",
    "webpack-cli": "^4.10.0",
    "webpack-dev-server": "^4.13.3"
  }
}
